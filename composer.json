{
	"name": "hofff/contao-layoutusage",
	"description": "Shows on which pages the layouts are used",
	"keywords": [ "layout", "page", "contao", "themes" ],
	"type": "contao-module",
	"license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Nicky Hoff",
			"email": "nick@hofff.com",
			"homepage": "https://www.hofff.com/",
			"role": "Maintainer"
		},
		{
			"name": "David Molineus",
			"email": "david@hofff.com",
			"homepage": "https://www.hofff.com/",
			"role": "Developer"
		},
		{
			"name": "Oliver Hoff",
			"email": "oliver@hofff.com",
			"homepage": "https://www.hofff.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/hofff/contao-layoutusage/issues",
		"source": "https://github.com/hofff/contao-layoutusage"
	},
	"require": {
		"php": "^8.1",
		"contao/core-bundle": "^4.13 || ^^5.3"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.1",
		"doctrine/coding-standard": "^12.0",
		"phpcq/runner-bootstrap": "^1.0@dev"
	},
	"conflict": {
		"contao/manager-plugin": "<2.1 || >= 3.0"
	},
	"replace": {
		"contao-legacy/backboneit_layoutusage": "self.version",
		"backboneit/contao-layoutusage": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Hofff\\Contao\\LayoutUsage\\": "src/"
		}
	},
	"extra": {
		"contao-manager-plugin": "Hofff\\Contao\\LayoutUsage\\ContaoManager\\Plugin"
	}
}
